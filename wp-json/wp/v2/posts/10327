0430\u0436\u0438<\/a>. \u0424\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043b\u0438\u0446\u043e \u0440\u0435\u0448\u0430\u0435\u0442 \u0441\u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u043e\u043a\u0430\u0442\u0430\u0442\u044c\u0441\u044f \u043f\u043e \u0415\u0432\u0440\u043e\u043f\u0435, \u0431\u043b\u0430\u0433\u043e \u0432\u0441\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0435 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b \u0434\u043b\u044f \u044d\u0442\u043e\u0433\u043e \u0443\u0436\u0435 \u0433\u043e\u0442\u043e\u0432\u044b, \u0430 \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u044b \u0432 \u041c\u043e\u0441\u043a\u0432\u0435 \u0432\u043f\u043e\u043b\u043d\u0435 \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u043f\u043e\u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c \u0438 \u043f\u043e\u0442\u043e\u043c \u0435\u0449\u0435 \u0438 \u043a\u0443\u043f\u0438\u0442\u044c \u043d\u0435\u043f\u043b\u043e\u0445\u043e\u0435 \u0436\u0438\u043b\u044c\u0435 \u043d\u0430 \u041a\u0430\u043c\u0447\u0430\u0442\u043a\u0435. \u041f\u043e\u0441\u043b\u0435 \u043c\u0435\u0441\u044f\u0446\u0430 \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u0439, \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442\u0441\u044f \u0432 \u0420\u043e\u0441\u0441\u0438\u044e \u0438 \u043e\u0442\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043d\u0430 \u043c\u0435\u0441\u0442\u043e \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f. \u041f\u043e \u043f\u0440\u0438\u0431\u044b\u0442\u0438\u0438, \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043e\u0441\u0442\u0430\u043d\u0430\u0432\u043b\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0432 \u043e\u0442\u0435\u043b\u0435 \u0438 \u0436\u0438\u0432\u0435\u0442 \u0442\u0430\u043c \u0435\u0449\u0435 \u043c\u0435\u0441\u044f\u0446, \u043f\