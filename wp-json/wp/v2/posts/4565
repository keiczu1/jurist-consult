42 \u0444\u0430\u043a\u0442, \u0447\u0442\u043e \u043d\u0430 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435 \u0440\u0435\u0431\u0435\u043d\u043a\u0430 \u0438\u0441\u0442\u0435\u0446 \u0442\u0440\u0430\u0442\u0438\u043b \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0432\u043e\u0438 \u0434\u0435\u043d\u044c\u0433\u0438 \u0438 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a \u043d\u0438\u0447\u0435\u043c \u043d\u0435 \u043f\u043e\u043c\u043e\u0433\u0430\u043b.<\/li>\n<\/ul>\n<h3>\u0420\u0430\u0441\u0445\u043e\u0434\u044b<\/h3>\n<p>\u0421\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u043f\u043f.2, \u043f.1 \u0441\u0442.333.36 \u041d\u041a \u0420\u0424, \u043f\u0440\u0438 \u043f\u043e\u0434\u0430\u0447\u0435 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0439, \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u0445 \u0441 \u0430\u043b\u0438\u043c\u0435\u043d\u0442\u043d\u044b\u043c\u0438 \u0432\u044b\u043f\u043b\u0430\u0442\u0430\u043c\u0438, \u0438\u0441\u0442\u0435\u0446 \u043d\u0435 \u043e\u0431\u044f\u0437\u0430\u043d \u043e\u043f\u043b\u0430\u0447\u0438\u0432\u0430\u0442\u044c \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u0443\u044e \u043f\u043e\u0448\u043b\u0438\u043d\u0443. \u0412 \u0442\u043e \u0436\u0435 \u0432\u0440\u0435\u043c\u044f, \u0435\u0441\u043b\u0438 \u043f\u043e\u043c\u0438\u043c\u043e \u0430\u043b\u0438\u043c\u0435\u043d\u0442\u043e\u0432 \u0432 \u0438\u0441\u043a\u043e\u0432\u043e\u043c \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0438 \u0431\u0443\u0434\u0443\u0442 \u0444\u0438\u0433\u0443\u0440\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f, \u0442\u043e \u0442\u043e\u0433\u0434\u0430 \u043f\u043e\u0448\u043b\u0438\u043d\u0430 \u0431\u0443\u0434\u0435\u0442 \u0443\u043f\u043b\u0430\u0447\u0438\u0432\u0430\u0442\u044c\u0441\u044f, \u043d\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u0440\u043