u0442 \u043b\u0438\u0448\u0435\u043d\u0438\u044f \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438, \u0436\u0438\u043b\u044c\u0446\u044b \u043e\u0442\u043a\u0430\u0436\u0443\u0442\u0441\u044f \u043f\u043e\u043a\u0438\u0434\u0430\u0442\u044c \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0443, \u043f\u0440\u0438\u0434\u0435\u0442\u0441\u044f \u043e\u0431\u0440\u0430\u0449\u0430\u0442\u044c\u0441\u044f \u043a \u0441\u0443\u0434\u0435\u0431\u043d\u044b\u043c \u043f\u0440\u0438\u0441\u0442\u0430\u0432\u0430\u043c. \u041e\u043d\u0438 \u0442\u043e\u0436\u0435 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0442 \u043d\u0435 \u043e\u0441\u043e\u0431\u043e \u0430\u043a\u0442\u0438\u0432\u043d\u043e, \u043e\u0434\u043d\u0430\u043a\u043e \u0435\u0441\u043b\u0438 \u0440\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u043e \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u0442\u044c \u0438\u043c \u043e \u0441\u0435\u0431\u0435 (\u0438 \u0438\u0445 \u043e\u0431\u044f\u0437\u0430\u043d\u043d\u043e\u0441\u0442\u044f\u0445), \u0432 \u043a\u043e\u043d\u0435\u0447\u043d\u043e\u043c \u0438\u0442\u043e\u0433\u0435 \u043c\u043e\u0436\u043d\u043e \u0434\u043e\u0431\u0438\u0442\u044c\u0441\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0432 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0443 \u0437\u0430\u044f\u0432\u044f\u0442\u0441\u044f \u043f\u0440\u0438\u0441\u0442\u0430\u0432\u044b \u0441 \u043f\u043e\u043b\u0438\u0446\u0438\u0435\u0439 \u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u044b\u0433\u043e\u043d\u044f\u0442 \u0436\u0438\u043b\u044c\u0446\u043e\u0432.<\/p>\n<h3>\u0418\u0441\u043a\u043e\u0432\u043e\u0435 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435<\/h3>\n<p>\u041e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442, \u0435\u0441\u043b\u0438 \u043d\u0435 \u0441\u0447\u0438\u0442\u0430\u0442\u044c \u0434\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432, \u044d\u0442\u043e \u0438\u0441\u043a\u043e\u0432\u043e\u0435 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435. \u0427\u0442\u043e\u0431\u044b \u0441\u0443\u0434 \u043f\u0440\u0438\u043d\u044f\u043b \u043d\u0443\u0436\u043d\u043e\u0435 \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0438 \u0432\u0441\u0435 \u0441\u0434\u0435\u043b\u0430\u043b \u0431\u044b\u0441\u0442\u0440\u043e, \u0438\u0441\u043a \u043e\u0431\u044f\u0437\u0430\u043d \u0431\u044b\u0442\u044c \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d \u0441 \u0443\u0447\u0435\u0442\u043e\u043c \u0432\u0441\u0435\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0445 \u043f\u0440\u0430\u0432\u0438\u043b, \u0437\u0430\u043a\u043e\u043d\u043e\u0432 \u0438 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439. \u041d\u0435\u0440\u0435\u0434\u043a\u043e \u0438\u0441\u0442\u0446\u044b \u0437\u0430\u043f\u043e\u043b\u043d\u044f\u044e\u0442 \u0442\u0430\u043a\u0438\u0435 \u0431\u0443\u043c\u0430\u0433\u0438 \u043d\u0435\u0432\u0435\u0440\u043d\u043e, \u0438\u0437-\u0437\u0430 \u0447\u0435\u0433\u043e \u0441\u0440\u043e\u043a \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u0438\u044f \u0434\u0435\u043b\u0430 \u0441\u0438\u043b\u044c\u043d\u043e \u0437\u0430\u0442\u044f\u0433\u0438\u0432\u0430\u0435\u0442\u0441\u044f, \u0430 \u0442\u043e \u0438 \u0432\u044b\u043d\u043e\u0441\u0438\u0442\u0441\u044f \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u043f\u043e\u043b\u043e\u0436\u043d\u043e\u0435 \u0436\u0435\u043b\u0430\u0435\u043c\u043e\u043c\u0443. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f \u043e\u0431\u0440\u0430\u0449\u0430\u0442\u044c\u0441\u044f \u043a \u043e\u043f\u044b\u0442\u043d\u044b\u043c \u044e\u0440\u0438\u0441\u0442\u0430\u043c \u0437\u0430 \u043f\u043e\u043c\u043e\u0449\u044c\u044e.<\/p>\n<h4>\u041a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0442\u044c<\/h4>\n<p>\u0418\u0441\u043a\u043e\u0432\u043e\u0435 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0432 \u0441\u0442\u0440\u043e\u0433\u043e\u043c \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438 \u0441\u043e <a href=\"httpss:\/\/www.consultant.ru\/document\/cons_doc_LAW_39570\/69470b15e4eb033f5bfa691094e1d083f32238e9\/\" target=\"_blank\" rel=\"noopener\">\u0441\u0442.131<\/a> \u0438 <a href=\"httpss:\/\/www.consultant.ru\/document\/cons_doc_LAW_39570\/c04bddd90fe8dd05299705ec675c65ecd0f668dc\/\" target=\"_blank\" rel=\"noopener\">132 \u0413\u041f\u041a \u0420\u0424<\/a>. \u0412 \u0442\u0430\u043a\u043e\u043c \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0435 \u0434\u043e\u043b\u0436\u043d\u0430 \u0444\u0438\u0433\u0443\u0440\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0430\u044f \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f:<\/p>\n<ul>\n<li>\u041d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043e\u0440\u0433\u0430\u043d\u0430 (\u0441\u0443\u0434\u0430), \u043a\u0443\u0434\u0430 \u043f\u043e\u0434\u0430\u0435\u0442\u0441\u044f \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435.<\/li>\n<li>\u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043e\u0431 \u0438\u0441\u0442\u0446\u0435, \u0435\u0433\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u0435\u043b\u0435 (\u0435\u0441\u043b\u0438 \u0435\u0441\u0442\u044c) \u0438 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0435. \u0422\u0443\u0442 \u0436\u0435 \u043c\u043e\u0433\u0443\u0442 \u0431\u044b\u0442\u044c \u0434\u0430\u043d\u043d\u044b\u0435 \u043e \u0442\u0440\u0435\u0442\u044c\u0438\u0445 \u043b\u0438\u0446\u0430\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e \u0442\u043e\u0439 \u0438\u043b\u0438 \u0438\u043d\u043e\u0439 \u043f\u0440\u0438\u0447\u0438\u043d\u0435 \u0437\u0430\u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u044b \u0432 \u0441\u0443\u0442\u0438 \u0432\u043e\u043f\u0440\u043e\u0441\u0430.<\/li>\n<li>\u041e\u0431\u0449\u0435\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b.<\/li>\n<li>\u0414\u0435\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u0439, \u0441\u043e \u0441\u0441\u044b\u043b\u043a\u0430\u043c\u0438 \u043d\u0430 \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u043c\u044b\u0435 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b.<\/li>\n<li>\u0422\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f \u043a \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430\u043c. \u0422\u0443\u0442 \u043d\u0443\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u043c\u0438, \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u0440\u0430\u0437\u043c\u044b\u0442\u044b\u0445 \u0444\u043e\u0440\u043c\u0443\u043b\u0438\u0440\u043e\u0432\u043e\u043a.<\/li>\n<li>\u041f\u0435\u0440\u0435\u0447\u0435\u043d\u044c \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u043c\u044b\u0445 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u043e\u0432.<\/li>\n<li>\u0414\u0430\u0442\u0430 \u0438 \u043f\u043e\u0434\u043f\u0438\u0441\u044c.<\/li>\n<\/ul>\n<h4>\u041e\u0431\u0440\u0430\u0437\u0435\u0446 \u0438\u0441\u043a\u0430<\/h4>\n<p><a href=\"httpss:\/\/ros-nasledstvo.com\/wp-content\/uploads\/2019\/08\/page1-4.jpg\"><img decoding=\"async\" class=\"aligncenter size-full wp-image-10403\" src=\"httpss:\/\/ros-nasledstvo.com\/wp-content\/uploads\/2019\/08\/page1-4.jpg\" alt=\"\" width=\"700\" height=\"517\" \/><\/a><\/p>\n<a href=\"httpss:\/\/ros-nasledstvo.com\/wp-content\/uploads\/2019\/08\/Skachat-obrazets-iska-o-prinuditelnoj-vypiske-iz-kvartiry-cherez-sud.doc\" class=\"btn_download icon_text\">\n        <span class=\"icn\"><img decoding=\"async\" src=\"https:\/\/ros-nasledstvo.com\/wp-content\/themes\/ros-nasledstvo\/images\/icon41.png\"><\/span>\n        <span class=\"txt\">\u0421\u043a\u0430\u0447\u0430\u0442\u044c \u043e\u0431\u0440\u0430\u0437\u0435\u0446 \u0438\u0441\u043a\u0430 \u043e \u043f\u0440\u0438\u043d\u0443\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0432\u044b\u043f\u0438\u0441\u043a\u0435 \u0438\u0437 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u044b \u0447\u0435\u0440\u0435\u0437 \u0441\u0443\u0434<\/span>\n        <span class=\"arrow\"><img decoding=\"async\" src=\"https:\/\/ros-nasledstvo.com\/wp-content\/themes\/ros-nasledstvo\/images\/icon42.png\"><\/span>\n    <\/a>\n<h3>\u0414\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b<\/h3>\n<p>\u041a \u0438\u0441\u043a\u043e\u0432\u043e\u043c\u0443 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u044e \u043d\u0443\u0436\u043d\u043e \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0442\u044c \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b:<\/p>\n<ul>\n<li>\u041f\u0430\u0441\u043f\u043e\u0440\u0442 \u0438\u0441\u0442\u0446\u0430.<\/li>\n<li>\u0414\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b, \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u044e\u0449\u0438\u0435 \u043f\u0440\u0430\u0432\u043e \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u043d\u0430 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0443. \u042d\u0442\u043e \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0434\u043e\u0433\u043e\u0432\u043e\u0440 \u043a\u0443\u043f\u043b\u0438-\u043f\u0440\u043e\u0434\u0430\u0436\u0438, \u0434\u0430\u0440\u0435\u043d\u0438\u044f, \u043f\u0440\u0438\u0432\u0430\u0442\u0438\u0437\u0430\u0446\u0438\u0438, \u0432\u044b\u043f\u0438\u0441\u043a\u0430 \u0438\u0437 \u0415\u0413\u0420\u041d \u0438 \u0442\u0430\u043a \u0434\u0430\u043b\u0435\u0435.<\/li>\n<li>\u041a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u044f \u043e\u0431 \u043e\u043f\u043b\u0430\u0442\u0435 \u0433\u043e\u0441\u043f\u043e\u0448\u043b\u0438\u043d\u044b.<\/li>\n<li>\u0414\u043e\u0432\u0435\u0440\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0438 \u043f\u0430\u0441\u043f\u043e\u0440\u0442 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u0435\u043b\u044f (\u0435\u0441\u043b\u0438 \u0442\u0440\u0435\u0431\u0443\u0435\u0442\u0441\u044f).<\/li>\n<li>\u041f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0435\u043d\u0438\u0435 \u0444\u0430\u043a\u0442\u0430 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0438 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430\u043c.<\/li>\n<li>\u0414\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u043d\u0430\u043b\u0438\u0447\u0438\u044f \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u0439 \u0441\u043e \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u043e\u0432 (\u043f\u043e\u0434\u0440\u043e\u0431\u043d\u0435\u0435 \u0441\u043c.\u043d\u0438\u0436\u0435).<\/li>\n<\/ul>\n<div class=\"scale_block\">\u0412 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439 \u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u0444\u0430\u043a\u0442\u043e\u0440\u043e\u0432 \u043f\u0435\u0440\u0435\u0447\u0435\u043d\u044c \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u043c\u043e\u0436\u0435\u0442 \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u0442\u044c\u0441\u044f \u0438\u043b\u0438 \u0432\u0438\u0434\u043e\u0438\u0437\u043c\u0435\u043d\u044f\u0442\u044c\u0441\u044f.<\/div>\n<h4>\u0412\u043e\u0437\u043c\u043e\u0436\u043d\u044b\u0435 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u044b \u0434\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432<\/h4>\n<p>\u0412\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u0432 \u0434\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e. \u041a \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u043c \u0438\u0437 \u043d\u0438\u0445 \u043c\u043e\u0436\u043d\u043e \u043e\u0442\u043d\u0435\u0441\u0442\u0438 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435:<\/p>\n<ul>\n<li>\u0421\u043f\u0440\u0430\u0432\u043a\u0430 \u0438\u0437 \u0416\u042d\u041a\u0430 (\u0443\u043f\u0440\u0430\u0432\u043b\u044f\u044e\u0449\u0435\u0439 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438) \u043e \u043d\u0430\u043b\u0438\u0447\u0438\u0438 \u0437\u0430\u0434\u043e\u043b\u0436\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0437\u0430 \u043a\u043e\u043c\u043c\u0443\u043d\u0430\u043b\u044c\u043d\u044b\u0435 \u043f\u043b\u0430\u0442\u0435\u0436\u0438. \u042d\u0442\u043e\u0442 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0434\u043e\u043b\u0436\u0435\u043d \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u0442\u044c\u0441\u044f \u0437\u0430\u0432\u0435\u0440\u0435\u043d\u043d\u043e\u0439 \u0432 \u0431\u0430\u043d\u043a\u0435 \u043f\u043b\u0430\u0442\u0435\u0436\u043a\u043e\u0439 \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u0441\u0430\u043c \u0437\u0430\u044f\u0432\u0438\u0442\u0435\u043b\u044c \u0441\u0432\u043e\u0438 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0432\u044b\u043f\u043e\u043b\u043d\u044f\u0435\u0442, \u0430 \u0434\u043e\u043b\u0433 \u0441\u0444\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043b\u0441\u044f \u043f\u043e \u0432\u0438\u043d\u0435 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430.<\/li>\n<li>\u0421\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u043e \u043e \u0440\u0430\u0441\u0442\