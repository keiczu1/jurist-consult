43d\u0430\u0436\u0438\u0442\u043e\u0435 \u0432 \u0431\u0440\u0430\u043a\u0435, \u043d\u043e \u043f\u043e\u0441\u043b\u0435 \u0442\u043e\u0433\u043e, \u043a\u0430\u043a \u0441\u0443\u043f\u0440\u0443\u0433\u0438 \u0444\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u043f\u0435\u0440\u0435\u0441\u0442\u0430\u043b\u0438 \u043f\u0440\u043e\u0436\u0438\u0432\u0430\u0442\u044c \u0432\u043c\u0435\u0441\u0442\u0435). \u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442: \u0438\u0441\u043a \u0438\u0441\u0442\u0446\u0430 \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u0447\u0430\u0441\u0442\u0438\u0447\u043d\u043e. \u0418\u0441\u043a \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430 \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e. \u041a\u0432\u0430\u0440\u0442\u0438\u0440\u0430 \u0431\u044b\u043b\u0430 \u0440\u0430\u0437\u0434\u0435\u043b\u0435\u043d\u0430 \u043d\u0430 \u0440\u0430\u0432\u043d\u044b\u0435 \u0434\u043e\u043b\u0438, \u0434\u043e\u043c \u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d \u0432 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430.<\/div>\n<div class=\"silver_block\"><strong>\u041f\u0440\u0438\u043c\u0435\u0440 \u21163<\/strong>: \u0418\u0441\u0442\u0435\u0446 \u0421\u0435\u043c\u0435\u043d\u043e\u0432 \u0412.\u0424. \u043f\u043e\u0434\u0430\u043b \u0438\u0441\u043a \u0432 \u0441\u0443\u0434 \u043a \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u0446\u0435 \u0421\u0435\u043c\u0435\u043d\u043e\u0432\u043e\u0439 \u041d.\u041f. \u0441 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043e \u0440\u0430\u0441\u0442\u043e\u0440\u0436\u0435\u043d\u0438\u0438 \u0431\u0440\u0430\u043a\u0430 \u0438 \u0440\u0430\u0437\u0434\u0435\u043b\u0435 \u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u0430 \u0440\u0430\u0432\u043d\u044b\u043c\u0438 \u0447\u0430\u0441\u0442\u044f\u043c\u0438. \u0421\u0435\u043c\u0435\u043d\u043e\u0432\u0430 \u041d.\u041f. \u043f\u043e\u0434\u0430\u043b\u0430 \u0432\u0441\u0442\u0440\u0435\u0447\u043d\u044b\u0439 \u0438\u0441\u043a \u0441 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0438\u0437\u043c\u0435\u043d\u0438\u0442\u044c \u043f\u0440\u0430\u0432\u0438\u043b\u0430 \u0440\u0430\u0437\u0434\u0435\u043b\u0430 \u0434\u043e\u043b\u0435\u0439, \u0443\u0447\u0438\u0442\u044b\u0432\u0430\u044f \u043d\u0430\u043b\u0438\u0447\u0438\u0435 \u043d\u0435\u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u043d\u043e\u043b\u0435\u0442\u043d\u0435\u0433\u043e \u0440\u0435\u0431\u0435\u043d\u043a\u0430. \u041e\u043f\u0438\u0440\u0430\u044f\u0441\u044c \u043d\u0430 \u043f\u0443\u043d\u043a\u0442 2 \u0441\u0442.39 \u0421\u041a \u0420\u0424 (\u043f\u043e\u0440\u044f\u0434\u043e\u043a \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u044f \u0434\u043e\u043b\u0435\u0439 \u043f\u0440\u0438 \u043d\u0430\u043b\u0438\u0447\u0438\u0438 \u043d\u0435\u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u043d\u043e\u043b\u0435\u0442\u043d\u0438\u0445 \u0434\u0435\u0442\u0435\u0439), \u0441\u0442.21 \u0421\u041a \u0420\u0424 (\u0440\u0430\u0437\u0432\u043e\u0434), \u0441\u0442.138 \u0413\u041f\u041a \u0420\u0424 (\u043e \u0432\u0441\u0442\u0440\u0435\u0447\u043d\u043e\u043c \u0438\u0441\u043a\u0435) \u0438 \u0421\u0442.38 \u0421\u041a \u0420\u0424 (\u0440\u0430\u0437\u0434\u0435\u043b \u0441\u043e\u0432\u043c\u0435\u0441\u0442\u043d\u043e\u0439 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438) \u043f\u043e\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043b \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u0435 \u0438\u0441\u0442\u0446\u0430 \u0447\u0430\u0441\u0442\u0438\u0447\u043d\u043e, \u0430 \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e, \u0440\u0430\u0437\u0434\u0435\u043b\u0438\u0432 \u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u043d\u0435 \u043d\u0430 \u0440\u0430\u0432\u043d\u044b\u0435 \u0434\u043e\u043b\u0438 \u043c\u0435\u0436\u0434\u0443 \u0441\u0443\u043f\u0440\u0443\u0433\u0430\u043c\u0438, \u0430 \u043d\u0430 3 \u0447\u0430\u0441\u0442\u0438, \u0432\u043a\u043b\u044e\u0447\u0438\u0432 \u0441\u044e\u0434\u0430 \u0438 \u043d\u0435\u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u043d\u043e\u043b\u0435\u0442\u043d\u0435\u0433\u043e \u0440\u0435\u0431\u0435\u043d\u043a\u0430.<\/div>\n<div class=\"quotes_block\">\n        <img decoding=\"async\" class=\"alignleft\" src=\"https:\/\/ros-nasledstvo.com\/wp-content\/themes\/ros-nasledstvo\/img\/img07.jpg\"><div class=\"quote\"><\/div>\u0412\u0441\u0442\u0440\u0435\u0447\u043d\u043e\u0435 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043e \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0442\u043e\u0447\u043d\u043e \u0438 \u0433\u0440\u0430\u043c\u043e\u0442\u043d\u043e, \u0438\u043d\u0430\u0447\u0435 \u0432 \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u0438\u0438 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u043e\u0442\u043a\u0430\u0437\u0430\u043d\u043e \u0438\u043b\u0438 \u0441\u0443\u0434 \u043d\u0435 \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f \u043e\u0442\u0432\u0435\u0442\u0447\u0438\u043a\u0430. \u041c\u044b \u043f\u043e\u043c\u043e\u0436\u0435\u043c \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0441\u043e \u0432\u0441\u0435\u043c\u0438 \u0441\u043f\u043e\u0440\u043d\u044b\u043c\u0438 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u043c\u0438, \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u044e\u0449\u0438\u0